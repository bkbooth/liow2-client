{
  "name": "liow2-client",
  "version": "2.0.0",
  "description": "The Love is our Weapon Campaign website client app",
  "keywords": [
    "Love",
    "Weapon",
    "Campaign",
    "website",
    "client",
    "app",
    "Angular",
    "ES6"
  ],
  "author": "Ben Booth <bkbooth@gmail.com>",
  "license": "MIT",
  "angular-cli": {},
  "repository": "git@github.com:loveisourweapon/liow2-client.git",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:dev": "ng build --dev",
    "build:prod": "ng build --prod --aot",
    "build": "npm run build:prod",
    "prebuild": "npm run lint && npm run test -- --single-run",
    "lint": "tslint \"src/**/*.ts\" --exclude \"src/testing/**\" --project src/tsconfig.json --type-check && tslint \"e2e/**/*.ts\" --project e2e/tsconfig.json --type-check",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^2.3.1",
    "@angular/compiler": "^2.3.1",
    "@angular/core": "^2.3.1",
    "@angular/forms": "^2.3.1",
    "@angular/http": "^2.3.1",
    "@angular/platform-browser": "^2.3.1",
    "@angular/platform-browser-dynamic": "^2.3.1",
    "@angular/router": "^3.3.1",
    "@ngrx/core": "^1.2.0",
    "@ngrx/effects": "^2.0.0",
    "@ngrx/router-store": "^1.2.5",
    "@ngrx/store": "^2.2.1",
    "bootstrap-sass": "^3.3.7",
    "core-js": "^2.4.1",
    "font-awesome": "^4.7.0",
    "lodash": "^4.17.4",
    "marked": "^0.3.6",
    "ng2-bootstrap": "^1.2.5",
    "ng2-ui-auth": "^6.1.1",
    "ngrx-store-freeze": "^0.1.6",
    "reselect": "^2.5.4",
    "rxjs": "^5.0.1",
    "seedrandom": "^2.4.2",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^2.3.1",
    "@types/jasmine": "2.5.38",
    "@types/lodash": "^4.14.50",
    "@types/marked": "^0.0.28",
    "@types/node": "^6.0.42",
    "@types/seedrandom": "^2.4.27",
    "angular-cli": "1.0.0-beta.26",
    "codelyzer": "~2.0.0-beta.1",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "protractor": "~4.0.13",
    "ts-node": "1.2.1",
    "tslint": "^4.3.0",
    "typescript": "~2.0.3"
  }
