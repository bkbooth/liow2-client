{
  "name": "liow2-client",
  "version": "1.11.0",
  "description": "The Love is our Weapon Campaign website client app",
  "private": true,
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:dev": "node server.js",
    "clean": "rimraf public/bundles/*",
    "prebuild": "npm run clean",
    "build": "webpack --progress",
    "build:prod": "NODE_ENV=production npm run build -- --optimize-minimize",
    "build:dev": "NODE_ENV=development npm run build -- --watch",
    "watch": "NODE_ENV=development npm start & npm run build:dev",
    "test": "mocha",
    "preversion": "npm run build:prod && git add --all public/bundles/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bkbooth/liow2-client.git"
  },
  "keywords": [
    "Love",
    "Weapon",
    "Campaign",
    "website",
    "client",
    "app",
    "Angular",
    "ES6"
  ],
  "author": "Ben Booth <bkbooth@gmail.com>",
  "license": "ISC",
